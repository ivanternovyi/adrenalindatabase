<!DOCTYPE html>
<html>
<head>
  <title>Adrenalindatabase</title>
  <%= favicon_link_tag 'favicon.ico' %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render 'layouts/header' %>

  <%= admins_access { render 'layouts/main_nav' } %>

  <%= render 'layouts/alert_notice' %>

  <div class="container_16">
    <%= yield %>
  </div>

  <%= render 'layouts/footer' %>

  <% if !session[:registration_notice].nil? %>
    <div id="registration_notice">
      <%= "#{session[:registration_notice]}" %>
    </div>
    <% session[:registration_notice] = nil %>
  <% end %>
</body>
</html>
