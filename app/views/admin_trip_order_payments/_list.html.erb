<% @payments.each do |top| %>
  <tr id=<%= "payment-#{top.id} payments-list" %>>
  <td>
    <%= "#{top.payment_date} : #{top.amount}" %>
    <%= link_to :edit_payment, edit_admin_user_trip_order_admin_trip_order_payment_path(@order, top), remote: true %>
    <%= link_to :delete_payment, admin_user_trip_order_admin_trip_order_payment_path(@order, top), method: :delete, remote: true, data: {confirm: :are_you_sure_to_delere_payment} %>
    </td>
  </tr>
<% end %>